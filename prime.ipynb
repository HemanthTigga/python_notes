   "source": [
    "n=int(input())\n",
    "c=1\n",
    "p=0\n",
    "while c<=n:\n",
    "    if n%c==0:\n",
    "        p=p+1\n",
    "    c=c+1\n",
    "if p==2:\n",
    "    print(\"prime\")\n",
    "else:\n",
    "    print(\"not prime\")"
   ]
